<%- include("./includes/head.ejs") %>
    <link rel="stylesheet" href="/css/users.css">
</head>
<body>
    <%- include("./includes/navigation.ejs") %>
    <div class="users">
        <h1>Users</h1>
        <% if (users.length > 0) {%>
            <ul class="user-list">
                <% for (let user of users) { %>
                    <li class="user-item"><%= user.username %></li>
                <% } %>
            </ul>
        <% } %>
        <h1>No Users Found</h1>
    </div>
<%- include("./includes/end.ejs") %>